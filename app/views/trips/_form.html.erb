<h2><%= page_title %></h2>




<main class="form">
  <%= form_for @trip, url: passenger_trips_path(params[:passenger_id])  do |f| %>

  <div class="form-fields">
    <%= f.label "Rating: "   %>
    <%= f.text_field :rating %>
  </div>


</br>

 <%= button_tag 'Clear Fields', :type => 'reset', class: "form-fields" %>

  <%= f.submit class: "form-fields", class: "submit-button" %>

  <%end%>
</main>
